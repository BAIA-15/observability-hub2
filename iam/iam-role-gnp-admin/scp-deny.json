{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyPurchases",
            "Effect": "Deny",
            "Action": [
                "ec2:PurchaseReserved*",
                "rds:PurchaseReserved*",
                "redshift:PurchaseReserved*",
                "elasticache:PurchaseReserved*",
                "es:PurchaseReserved*",
                "aws-marketplace:*"
            ],
            "Resource": "*"
        }
    ]
}